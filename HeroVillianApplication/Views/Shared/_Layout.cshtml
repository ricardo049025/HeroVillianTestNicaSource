<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="~/Content/css/materialize.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/css/app.css" rel="stylesheet" type="text/css" />
    <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
    @*<script src="~/Content/js/materialize.min.js"></script>*@
    @*@Scripts.Render("~/bundles/modernizr")*@
</head>
<body>

    <div class="">

        <div class="container-fluid nav">
            <div class="row">
                <nav id="navbarNica" class="transparent">
                    <div class="nav-wrapper">
                        <a class="left" href="/">NicaSource</a>
                    </div>
                </nav>
            </div>
        </div>


        <div class="">
            @RenderBody()
            <hr />
            <footer>
                <p>&copy; @DateTime.Now.Year - NicaSource "&" Marvel </p>
            </footer>
        </div>

    </div>



    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Content/js/materialize.min.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
